{
  "name": "astro-place-backend",
  "version": "1.0.0",
  "description": "Backend practice with Node.js and Express.js",
  "main": "src/index.js",
  "engines": {
    "node": ">=14 <18"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:rollback": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "vercel-build": "echo deployed successfully"
  },
  "keywords": [],
  "author": "Bryan Garay <bryangarayacademico@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.10.1",
    "mysql2": "^3.6.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "save": "^2.9.0",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1"
  }
}
